{
  "manifest_version": 2,
  "name": "App",
  "description": "Description of App",
  "version": "1.0",
  "background": {
    "scripts": [
      "js/contextMenu.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/square-app-512.png",
    "default_popup": "popup.html",
    "default_title": "App"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
